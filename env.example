# Configuración de Google Maps API
# Necesitas obtener una API key desde Google Cloud Console
# https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=tu-api-key-aqui

# Configuración de Supabase
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_API_KEY=tu-anon-key-aqui

# Configuración de la base de datos PostgreSQL
DATABASE_URL=postgresql://usuario:contraseña@localhost:5432/nombre_db

# Configuración de Supabase (si usas Supabase)
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_ANON_KEY=tu-anon-key-aqui
SUPABASE_SERVICE_ROLE_KEY=tu-service-role-key-aqui

# Configuración alternativa de base de datos (si no usas DATABASE_URL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nombre_db
DB_USER=usuario
DB_PASSWORD=contraseña
